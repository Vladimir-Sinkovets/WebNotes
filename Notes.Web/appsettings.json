{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=Notes;Trusted_Connection=True"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "MarkdownSharpOptions": {
    "AllowTargetBlank": "true",
    "AllowEmptyLinkText": "true",
    "DisableImages": "true",
    "QuoteSingleLine": "false",
    "AutoHyperlink": "true",
    "AutoNewLines": "true",
    "LinkEmails": "true",
    "StrictBoldItalic": "true",
    "AsteriskIntraWordEmphasis": "true",
    "LinkEmailsWithoutAngleBrackets": "true"
  },
  "AllowedHosts": "*"
}
